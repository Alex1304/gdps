{% extends 'base.html.twig' %}

{% block content %}
	{% for player in playerList %}
		1:{{ player.name }}:
		2:{{ player.id }}:
		13:{{ player.coins }}:
		17:{{ player.userCoins }}:
		6:{{ loop.index0 + rankOffset }}:
		9:{{ player.icon }}:
		10:{{ player.color1 }}:
		11:{{ player.color2 }}:
		14:{{ player.iconType }}:
		15:{{ player.special }}:
		16:{% if player.account %}{{ player.account.id }}{% else %}0{% endif %}:
		3:{{ player.stars }}:
		8:{{ player.creatorPoints }}:
		4:{{ player.demons }}
		{% if not player.account %}:7:{{ player.deviceID }}{% endif %}{% if not loop.last %}|{% endif %}
	{% endfor %}
{% endblock %}