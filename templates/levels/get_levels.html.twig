{% extends 'base.html.twig' %}

{% block content %}
	{% for level in levels %}
		1:{{ level.id }}:
		2:{{ level.name }}:
		5:{{ level.version }}:
		6:{% if level.creator %}{{ level.creator.id }}{% else %}0{% endif %}:
		8:10:
		9:{{ level.difficulty * 10 }}:
		10:{{ level.downloadedBy|length }}:
		12:{{ level.audioTrack }}:
		13:{{ level.gameVersion }}:
		14:{{ level.likedBy|length - level.dislikedBy|length }}:
		17:{% if level.isDemon %}1{% else %}0{% endif %}:
		43:{{ level.demonDifficulty }}:
		25:{% if level.isAuto %}1{% else %}0{% endif %}:
		18:{{ level.stars }}:
		19:{{ level.featureScore }}:
		42:{% if level.isEpic %}1{% else %}0{% endif %}:
		45:{{ level.objectCount }}:
		3:{{ level.description }}:
		15:{{ level.length }}:
		30:{% if level.original %}{{ level.original.id }}{% else %}0{% endif %}:
		31:0:
		37:{{ level.coins }}:
		38:{% if level.hasCoinsVerified %}1{% else %}0{% endif %}:
		39:{{ level.requestedStars }}:
		46:1:
		47:2:
		40:{% if level.isLDM %}1{% else %}0{% endif %}:
		35:{{ level.customSongID }}
		{% if not loop.last %}|{% endif %}
	{% endfor %}
	#
	{% for creator in creators %}
		{{ creator.playerID }}:{{ creator.name }}:{{ creator.accountID }}
		{% if not loop.last %}|{% endif %}
	{% endfor %}
	#
	{% for song in songs %}
		1~|~{{ song.id }}~|~
		2~|~{{ song.name }}~|~
		3~|~{{ song.authorID }}~|~
		4~|~{{ song.authorName }}~|~
		5~|~{{ song.size }}~|~
		6~|~~|~
		7~|~{{ song.download }}~|~
		8~|~0
		{% if not loop.last %}~:~{% endif %}
	{% endfor %}
	#
	{{ total }}:{{ page }}:10
	#
	{{ hash }}
{% endblock %}