{% extends 'base.html.twig' %}

{% block content %}
	{% for score in scores %}
		1:{{ score.account.username }}:
		2:{{ score.account.player.id }}:
		9:{{ score.account.player.icon }}:
		10:{{ score.account.player.color1 }}:
		11:{{ score.account.player.color2 }}:
		14:{{ score.account.player.iconType }}:
		15:{{ score.account.player.special }}:
		16:{{ score.account.id }}:
		3:{{ score.percent }}:
		6:{{ loop.index }}:
		13:{{ score.coins }}:
		42:{{ timeFormatter.format(score.updatedAt) }}
	{% if not loop.last %}|{% endif %}
	{% endfor %}
{% endblock %}